#include <iostream>
using namespace std;

const int MAX = 10001;

// is it possible to make this amount in change
int amount[MAX][100];

int bills[100];

int cost;
int num_bills;


void fillArray() {

   for(int i = 1; i < MAX; i++) {
      for(int j = 0; j < num_bills; j++) {
	 for(int k = 0; k < j; k++) {
	    if(i - bills[k] < 0) continue;
	    if(amount[i - bills[k]][j - 1] != -1) {
	       amount[i][j] =  1 + amount[i - bills[k]][j - 1]);
	 }
      }
	 cout << amount[i][j] << " " ;
      }
      cout << endl;
   }

}

int main() {

   int cases;
   cin >> cases;
   for(int i = 0; i < cases; i++) {
      for(int j =0; j < MAX; j++) {
	 fill(amount[i], amount[i] + 100, -1);
      }
      cin >> cost;
      cin >> num_bills;
      for(int j = 0; j < num_bills; j++) {
	 cin >> bills[j];
      }
      amount[0][0] = 0;
      // do something
      fillArray();
      
      for(int i = cost; i < MAX; i++) {
	 if(amount[i][num_bills - 1] != -1) {
	    cout << i << " " << amount[i][num_bills - 1] << endl;
	    break;
	 }
      }
      
   }
   
   
   return 0;
}
